{
	"organization": "gearboxworks",
	"name": "mysql",
	"version": "5.7.25",
	"majorversion": "5.7",
	"latest": "",
	"class": "database",
	"network": "--network gearboxnet",
	"ports": "-p 3306:3306",
	"volumes": "-v /opt/box/provision:/docker-entrypoint-initdb.d -v mysql-5.7:/var/lib/mysql",
	"restart": "--restart no",
	"args": "-e MYSQL_ALLOW_EMPTY_PASSWORD=yes -e MYSQL_DATABASE=wordpress -e MYSQL_USER=wordpress -e MYSQL_PASSWORD=wordpress",
	"env": "MYSQL_VERSION"
}

